{
    "name": "emag-tech-labs/messenger-mongo-bundle",
    "description": "A Mongo transport for the Symfony Messenger component",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Iosif Chiriluta",
            "email": "iosifpub@gmail.com",
            "role": "Developer"
        },
        {
            "name": "eMAGTechLabs",
            "email": "techlabs@emag.ro",
            "homepage": "http://techlabs.emag.ro",
            "role": "Maintainer"
        }
